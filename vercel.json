{
	"version": 2,
	"builds": [
		{ "src": "**/*.html", "use": "@vercel/static" },
		{ "src": "**/*.json", "use": "@vercel/static" },
		{ "src": "script.js", "use": "@vercel/static" },
		{ "src": "style.css", "use": "@vercel/static" },
		{ "src": "images/**", "use": "@vercel/static" }
	],
	"routes": [
		{ "src": "/", "dest": "/home.html" },
		{ "src": "/(.*)", "dest": "/$1" }
	],
	"headers": [
		{
			"source": "/(.*).json",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/json"
				}
			]
		}
	]
}
