{
	"rewrites": [
    { "source": "/", "destination": "/pages/home.html" },
    { "source": "/(.*).html", "destination": "/pages/$1.html" },
    { "source": "/(.*)", "destination": "/pages/$1.html" }
  	],
	"headers": [
		{
			"source": "/(.*).json",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/json"
				}
			]
		},
		{
			"source": "/(.*).js",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/javascript"
				}
			]
		}
	]
}
