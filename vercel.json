{
	"rewrites": [{ "source": "/", "destination": "pages/home.html" }],
	"headers": [
		{
			"source": "/(.*).json",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/json"
				}
			]
		},
		{
			"source": "/(.*).js",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/javascript"
				}
			]
		}
	]
}
